	<table>
		<tr><th>Directory</th><th>Contents</th>
		<tr>
			<td>/bin</td>
			<td>Command binaries </td>
		</tr>

		<tr>
			<td>/home </td>
			<td>Users home directories</td>
		</tr>

		<tr>
			<td> /lib</td>
			<td> System libraries</td>
		</tr>

		<tr>
			<td>/sbin </td>
			<td> System binaries</td>
		</tr>

		<tr>
			<td>/tmp </td>
			<td> Scratch Space </td>
		</tr>

		<tr>
			<td> /usr/bin</td>
			<td>Binaries for user commands </td>
		</tr>

		<tr>
			<td>/usr/include </td>
			<td> Header or include files</td>
		</tr>

		<tr>
			<td>/usr/lib </td>
			<td>Libraries  </td>
		</tr>

		<tr>
			<td> /usr/local</td>
			<td>Locally installed binaries,libraries,and includes </td>
		</tr>
	</table>
